# =============================================================================
# Code Server with AI - Environment Configuration
# =============================================================================
# Copy this file to .env and configure your settings
# =============================================================================

CONTAINER_NAME=code-server-with-ai
IMAGE_NAME=code-server-with-ai:latest

HOST_PORT_VSCODE=8585
HOST_PORT_OPENCODE=4096
HOST_PORT_OPENCLAW=18789

VSCODE_HOST=0.0.0.0
VSCODE_PORT=8585
VSCODE_TOKEN=your-secure-token-here
VSCODE_EXTRA_ARGS=

USE_CDN_PROXY=false
CDN_PROXY_HOST=

OPENCODE_HOST=0.0.0.0
OPENCODE_PORT=4096
OPENCODE_SERVER_USERNAME=opencode
OPENCODE_SERVER_PASSWORD=your-opencode-password

OPENCLAW_PORT=18789
OPENCLAW_GATEWAY_TOKEN=your-openclaw-token

PUID=1000
PGID=1000
EXTRA_GID=

DATA_DIR=./data
